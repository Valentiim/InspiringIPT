@model IEnumerable<InspiringIPT.Models.Cursos>


<h2></h2>
<!-- Header Carousel -->
<header id="entrada" class="carousel slide">
    @if (TempData["LogSuccess"] != null)
    {
        <div id="myModal" class="modal fade" role="dialog">
            <div class="modal-dialog">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header modal-header-success">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">@TempData["is"]</h4>
                    </div>
                    <div class="modal-body">
                        <p>@TempData["LogSuccess"]</p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-success" data-dismiss="modal">Fechar</button>
                    </div>
                </div>
            </div>
        </div>
    }
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#entrada" data-slide-to="0" class="active"></li>
        <li data-target="#entrada" data-slide-to="1"></li>
        <li data-target="#entrada" data-slide-to="2"></li>
        <li data-target="#entrada" data-slide-to="3"></li>
    </ol>
    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="~/Imags/cara_ipt2.jpg" alt="Chania" />
            <div class="carousel-caption">
            </div>
        </div>
        <div class="item">
            <img src="~/Imags/cara_ipt2.jpg" alt="Chania" />
            <div class="carousel-caption">
            </div>
        </div>

        <div class="item">
            <img src="~/Imags/cara_ipt2.jpg" alt="Chania" />
            <div class="carousel-caption">
            </div>
        </div>

        <div class="item">
            <img src="~/Imags/cara_ipt2.jpg" alt="Chania" />
            <div class="carousel-caption">
            </div>
        </div>
    </div>

    <!-- Left and right controls -->
    <a class="left carousel-control" href="#entrada" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#entrada" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
